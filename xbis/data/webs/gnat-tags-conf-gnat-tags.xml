<?xml version="1.0" encoding="ISO-8859-1"?>
<taglib>
    <tlibversion>1.0</tlibversion>
    <jspversion>1.1</jspversion>
    <shortname>gnat</shortname>
    <uri>WEB-INF/lib/gnat-tags.jar</uri>
    <tag>
        <name>deltree</name>
        <tagclass>org.apache.taglibs.gnat.deltreeTag</tagclass>
        <bodycontent>empty</bodycontent>
        <attribute>
            <name>dir</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>echo</name>
        <tagclass>org.apache.taglibs.gnat.echoTag</tagclass>
        <bodycontent>jsp</bodycontent>
        <attribute>
            <name>message</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>file</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>append</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>fail</name>
        <tagclass>org.apache.taglibs.gnat.exitTag</tagclass>
        <bodycontent>empty</bodycontent>
        <attribute>
            <name>message</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>gzip</name>
        <tagclass>org.apache.taglibs.gnat.gzipTag</tagclass>
        <bodycontent>empty</bodycontent>
        <attribute>
            <name>src</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>zipfile</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>gunzip</name>
        <tagclass>org.apache.taglibs.gnat.gunzipTag</tagclass>
        <bodycontent>empty</bodycontent>
        <attribute>
            <name>src</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>dest</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>ls</name>
        <tagclass>org.apache.taglibs.gnat.lsTag</tagclass>
        <teiclass>org.apache.taglibs.gnat.lsTEI</teiclass>
        <bodycontent>jsp</bodycontent>
        <attribute>
            <name>dir</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>id</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>mkdir</name>
        <tagclass>org.apache.taglibs.gnat.mkdirTag</tagclass>
        <bodycontent>empty</bodycontent>
        <attribute>
            <name>dir</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>clobber</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>touch</name>
        <tagclass>org.apache.taglibs.gnat.touchTag</tagclass>
        <bodycontent>empty</bodycontent>
        <attribute>
            <name>file</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>millis</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>datetime</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>tstamp</name>
        <tagclass>org.apache.taglibs.gnat.tstampTag</tagclass>
        <teiclass>org.apache.taglibs.gnat.tstampTEI</teiclass>
        <bodycontent>empty</bodycontent>
    </tag>
</taglib>
